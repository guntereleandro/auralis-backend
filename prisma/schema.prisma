generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  // N√ÉO TEM url aqui!
}

model User {
  id           Int      @id @default(autoincrement())
  email        String   @unique
  fullName     String
  displayName  String?
  password     String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}